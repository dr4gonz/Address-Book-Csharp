<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Address Book</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
        <link rel="stylesheet" href="Content/css/styles.css" type="text/css">
    </head>
    <body>
        <h1>View All Contacts</h1>
        @foreach (var contact in Model)
        {
            <ul>
                <h3>@contact.GetName()</h3>
                <h4>Phone Number: @contact.GetPhoneNumber()</h4>
                <h4>Address: @contact.GetAddress()</h4>
                <h4>City, State, Zip: @contact.GetCity(), @contact.GetState(), @contact.GetZip()</h4>
            </ul>
        }
    </body>
</html>
